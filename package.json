{
  "name": "koda",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "cli": "KODA_MODE=cli-only bun run src/index.ts",
    "koda": "bun run src/cli.ts",
    "test": "bun test",
    "bench": "bun run bench/runner.ts --suite deterministic",
    "bench:deterministic": "bun run bench/runner.ts --suite deterministic",
    "bench:judge": "bun run bench/runner.ts --suite llm-judge",
    "bench:all": "bun run bench/runner.ts --suite all",
    "bench:ci": "bun run bench/runner.ts --suite deterministic --format json",
    "bench:full": "bun run bench/runner.ts --suite all --format json"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.0.0",
    "@clack/prompts": "^0.9.1",
    "@openrouter/ai-sdk-provider": "^2.1.1",
    "@tavily/core": "^0.7.1",
    "ai": "^6.0.0",
    "chalk": "^5.4.1",
    "grammy": "^1.39.0",
    "ora": "^8.2.0",
    "supermemory": "^4.0.0",
    "zod": "^4.3.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.9.0",
    "@types/bun": "^1.3.8"
  }
}
